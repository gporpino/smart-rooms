<table class="table table-striped table-hover rooms">
  <thead>
    <tr>
      <th>Name</th>
      <th>Owner</th>
    </tr>
  </thead>

  <tbody>
    <% rooms.each do |room| %>
      <tr data-link="<%= room_path(room) %>" class='table_row'>
        <td><%= room.name %></td>
        <td><%= room.owner.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate rooms %>
